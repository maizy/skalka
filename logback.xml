<configuration debug="true">
    <appender name="STDERR" class="ch.qos.logback.core.ConsoleAppender" target="System.err">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %thread %.-1level [%logger{36}]: %msg%n</pattern>
        </encoder>
    </appender>

    <root level="debug">
        <appender-ref ref="STDERR"/>
    </root>
</configuration>
